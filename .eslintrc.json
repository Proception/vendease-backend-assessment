{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb-typescript-prettier",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "camelcase": "off",
    "@typescript-eslint/camelcase": "off"
  }
}